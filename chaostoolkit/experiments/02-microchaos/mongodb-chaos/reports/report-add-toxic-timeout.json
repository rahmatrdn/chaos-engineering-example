  "experiment": {
    "version": "1.0.0",
    "title": "MongoDB timeout toxic via chaostoxi",
    "description": "This experiment injects a timeout toxic into the MongoDB proxy using chaostoxi to simulate database timeouts and observe the impact on the microservices.",
    "configuration": {
      "toxiproxy_host": "toxiproxy",
      "toxiproxy_port": 8474
    },
    "runtime": {
      "rollbacks": {
        "strategy": "always"
      }
    },
    "steady-state-hypothesis": {
      "title": "Services may degrade",
      "probes": [
        {
          "type": "probe",
          "name": "order-api-health",
          "tolerance": [
            200
          ],
          "provider": {
            "type": "http",
            "timeout": 15,
            "url": "http://order-api:8081/health",
            "method": "GET"
          }
        },
        {
          "type": "probe",
          "name": "notifier-health",
          "tolerance": [
            200
          ],
          "provider": {
            "type": "http",
            "timeout": 15,
            "url": "http://notifier:8082/health",
            "method": "GET"
          }
        }
      ]
    },
    "method": [
      {
        "type": "action",
        "name": "add-timeout-mongo",
        "provider": {
          "type": "python",
          "module": "chaostoxi.toxic.actions",
          "func": "create_timeout_toxic",
          "arguments": {
            "for_proxy": "mongo_proxy",
            "toxic_name": "mongo_timeout",
            "timeout": 5000
          }
        },
        "pauses": {
          "after": 1
        }
      },
      {
        "type": "action",
        "name": "create-order",
        "provider": {
          "type": "http",
          "timeout": 20,
          "url": "http://order-api:8081/orders",
          "method": "POST",
          "headers": {
            "Content-Type": "application/json"
          },
          "arguments": {
            "customer": "mongo-timeout",
            "item": "kopi",
            "qty": 1
          }
        }
      },
      {
        "type": "action",
        "name": "remove-timeout-mongo",
        "provider": {
          "type": "python",
          "module": "chaostoxi.toxic.actions",
          "func": "delete_toxic",
          "arguments": {
            "for_proxy": "mongo_proxy",
            "toxic_name": "mongo_timeout"
          }
        }
      }
    ],
    "rollbacks": [
      {
        "type": "action",
        "name": "remove-timeout-mongo",
        "provider": {
          "type": "python",
          "module": "chaostoxi.toxic.actions",
          "func": "delete_toxic",
          "arguments": {
            "for_proxy": "mongo_proxy",
            "toxic_name": "mongo_timeout"
          }
        }
      }
    ],
    "dry": null
  },
