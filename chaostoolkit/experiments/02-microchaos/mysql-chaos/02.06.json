          "type": "probe",
          "name": "notifier-health",
          "tolerance": [
            200
          ],
          "provider": {
            "type": "http",
            "timeout": 15,
            "url": "http://notifier:8082/health",
            "method": "GET"
          }
        }
      ]
    },
    "method": [
      {
        "type": "action",
        "name": "add-slicer-mysql",
        "provider": {
          "type": "python",
          "module": "chaostoxi.toxic.actions",
          "func": "create_slicer_toxic",
          "arguments": {
            "for_proxy": "mysql_proxy",
            "toxic_name": "mysql_slicer",
            "average_size": 32,
            "size_variation": 16,
            "delay": 0
          }
        },
        "pauses": {
          "after": 1
        }
      },
      {
        "type": "probe",
        "name": "create-order-expected",
        "tolerance": [
          201
        ],
        "provider": {
          "type": "http",
          "timeout": 20,
          "url": "http://order-api:8081/orders",
          "method": "POST",
          "headers": {
            "Content-Type": "application/json"
          },
          "arguments": {
            "customer": "mysql-slicer",
            "item": "kopi",
            "qty": 1
          }
        }
      },
      {
        "type": "action",
        "name": "remove-slicer-mysql",
        "provider": {
          "type": "python",
          "module": "chaostoxi.toxic.actions",
          "func": "delete_toxic",
