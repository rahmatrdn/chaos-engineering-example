  "experiment": {
    "version": "1.0.0",
    "title": "RabbitMQ slicer toxic via chaostoxi",
    "description": "This experiment injects a slicer toxic into the RabbitMQ proxy using chaostoxi to simulate packet fragmentation and observe the impact on the microservices.",
    "configuration": {
      "toxiproxy_host": "toxiproxy",
      "toxiproxy_port": 8474
    },
    "runtime": {
      "rollbacks": {
        "strategy": "always"
      }
    },
    "steady-state-hypothesis": {
      "title": "Services may degrade",
      "probes": [
        {
          "type": "probe",
          "name": "order-api-health",
          "tolerance": [
            200
          ],
          "provider": {
            "type": "http",
            "timeout": 15,
            "url": "http://order-api:8081/health",
            "method": "GET"
          }
        },
        {
          "type": "probe",
          "name": "notifier-health",
          "tolerance": [
            200
          ],
          "provider": {
            "type": "http",
            "timeout": 15,
            "url": "http://notifier:8082/health",
            "method": "GET"
          }
        }
      ]
    },
    "method": [
      {
        "type": "action",
        "name": "add-slicer-rabbitmq",
        "provider": {
          "type": "python",
          "module": "chaostoxi.toxic.actions",
          "func": "create_slicer_toxic",
          "arguments": {
            "for_proxy": "rabbitmq_proxy",
            "toxic_name": "rabbitmq_slicer",
            "average_size": 32,
            "size_variation": 16,
            "delay": 0
          }
        },
        "pauses": {
          "after": 1
        }
      },
      {
        "type": "action",
        "name": "create-order",
        "provider": {
          "type": "http",
          "timeout": 20,
          "url": "http://order-api:8081/orders",
          "method": "POST",
          "headers": {
            "Content-Type": "application/json"
          },
          "arguments": {
            "customer": "rabbitmq-slicer",
            "item": "kopi",
            "qty": 1
          }
        }
      },
      {
        "type": "action",
        "name": "remove-slicer-rabbitmq",
        "provider": {
          "type": "python",
          "module": "chaostoxi.toxic.actions",
          "func": "delete_toxic",
          "arguments": {
            "for_proxy": "rabbitmq_proxy",
            "toxic_name": "rabbitmq_slicer"
          }
        }
      }
    ],
    "rollbacks": [
      {
        "type": "action",
        "name": "remove-slicer-rabbitmq",
        "provider": {
          "type": "python",
          "module": "chaostoxi.toxic.actions",
          "func": "delete_toxic",
          "arguments": {
            "for_proxy": "rabbitmq_proxy",
            "toxic_name": "rabbitmq_slicer"
          }
        }
      }
    ],
    "dry": null
  },
